{
  "name": "todo-list",
  "version": "0.1.0",
  "private": true,
  "description": "A todo list app with progressive enhancement",
  "author": "Mathias Sch√§fer <zapperlott@9elements.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec babel-node -- src/server.js",
    "clean": "rimraf dist",
    "build:production": "npm run clean && npm run build:webpack:production",
    "build:development": "npm run clean && npm run build:webpack:development",
    "build:webpack:production": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "build:webpack:development": "cross-env NODE_ENV=development webpack --config webpack.config.development.js",
    "watch:webpack": "webpack --watch --config webpack.config.development.js",
    "lint": "eslint src"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "body-parser": "^1.14.2",
    "classnames": "^2.2.1",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "ejs": "^2.3.4",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.13.1",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "leveldown": "^1.4.3",
    "levelup": "^1.3.1",
    "lodash": "^3.10.1",
    "nodemon": "^1.8.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc4",
    "redux": "^3.0.5",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "uuid": "^2.0.1",
    "webpack": "^1.12.9"
  }
}
