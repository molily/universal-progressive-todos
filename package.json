{
  "name": "universal-progressive-todos",
  "version": "0.1.0",
  "private": true,
  "description": "A todo list app with progressive enhancement",
  "author": "Mathias Sch√§fer <zapperlott@9elements.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run prepare-dist && npm run build:css && npm run server",
    "start:production": "cross-env NODE_ENV=production npm run build:production && npm run server",
    "server": "nodemon --exec babel-node --watch src --ext js src/server.js",
    "clean": "rimraf dist",
    "prepare-dist": "npm run clean && mkdirp dist/static/",
    "build:production": "npm run prepare-dist && npm run build:css && npm run build:webpack:production",
    "build:development": "npm run prepare-dist && npm run build:css && npm run build:webpack:development",
    "build:webpack:production": "cross-env NODE_ENV=production webpack",
    "build:webpack:development": "cross-env NODE_ENV=development webpack",
    "build:css": "node-sass --output-style expanded src/stylesheets/main.scss dist/static/main.css",
    "lint": "eslint src"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "levelup": "^1.3.2",
    "lodash": "^4.15.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-lodash": "^3.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "classnames": "^2.2.5",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "ejs": "^2.5.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "history": "^4.0.0-2",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.10.2",
    "react-router": "^2.7.0",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
