{
  "name": "universal-progressive-todos",
  "version": "0.1.0",
  "private": true,
  "description": "A todo list app with progressive enhancement",
  "author": "Mathias Sch√§fer <molily@mailbox.org>",
  "license": "MIT",
  "scripts": {
    "test": "node src/test.js",
    "start": "npm run start:development",
    "start:development": "cross-env-shell NODE_ENV=development \"npm run prepare-dist && npm run build:css && npm run server:development\"",
    "start:production": "cross-env-shell NODE_ENV=production \"npm run build:client:production && npm run build:server:production && npm run server:production\"",
    "server:development": "nodemon --exec babel-node --watch src --ext js --delay 2 src/server.js",
    "server:production": "node dist/server.js",
    "prepare-dist": "rimraf dist && mkdirp dist/static/",
    "build": "npm run build:client:production",
    "build:client:development": "npm run prepare-dist && npm run build:css && npm run build:webpack:development",
    "build:client:production": "npm run prepare-dist && npm run build:css && npm run build:webpack:production",
    "build:server:production": "babel src --out-dir dist",
    "build:webpack:development": "cross-env NODE_ENV=development webpack",
    "build:webpack:production": "cross-env NODE_ENV=production webpack",
    "build:css": "node-sass --output-style expanded src/stylesheets/main.scss dist/static/main.css",
    "lint": "eslint src"
  },
  "_moduleAliases": {
    "react": "./node_modules/preact/compat/dist/compat.js",
    "react-dom": "./node_modules/preact/compat/dist/compat.js"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "ejs": "^2.6.1",
    "encoding-down": "^6.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "express": "^4.17.1",
    "leveldown": "^5.1.0",
    "levelup": "^4.0.1",
    "mkdirp": "^0.5.1",
    "module-alias": "^2.2.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "preact": "next",
    "preact-render-to-string": "next",
    "prop-types": "^15.7.2",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-transform-hmr": "^1.0.4",
    "redux": "^4.0.1",
    "rimraf": "^2.6.3",
    "uuid": "^3.3.2",
    "webpack": "4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
