{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 1%"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    ]
  ],

  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-transform-react-constant-elements",
        [
          "@babel/plugin-transform-runtime",
          {
            "regenerator": false
          }
        ]
      ]
    }
  },
  "exclude": "node_modules/**"
}
